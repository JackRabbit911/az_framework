{
    "name": "az/az_framework",
    "description": "Simple lightweight modern component psr compatible PHP framework",
    "keywords": ["php", "psr", "framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alexey Zaytsev",
            "email": "world.zay@gmail.com"
        }
    ],
    "require": {
        "php-di/php-di": "^7.0",
        "monolog/monolog": "^3.3",
        "httpsoft/http-message": "^1.0",
        "httpsoft/http-server-request": "^1.0",
        "httpsoft/http-runner": "^1.0",
        "httpsoft/http-response": "^1.0",
        "pecee/pixie": "^4.15",
        "behat/transliterator": "^1.5",
        "twig/twig": "^3.5",
        "filp/whoops": "^2.15",
        "league/climate": "^3.8",
        "symfony/yaml": "^6.2",
        "vlucas/phpdotenv": "^5.5",
        "erusev/parsedown": "^1.7",
        "dragonmantank/cron-expression": "^3.3",
        "phpmailer/phpmailer": "^6.8",
        "symfony/process": "^6.3",
        "guzzlehttp/guzzle": "^7.8",
        "nette/utils": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Az\\Route\\": "vendor/az/route/src/",
            "Az\\Session\\": "vendor/az/session/src/",
            "Az\\Validation\\": "vendor/az/validation/src/",
            "Sys\\": "vendor/az/sys/src/"
        }
    },
    "require-dev": {
        "fakerphp/faker": "^1.22",
        "ottaviano/faker-gravatar": "^0.2.0",
        "phpunit/phpunit": "^11",
        "mockery/mockery": "^1.6"
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\Az\\Route\\": "vendor/az/route/tests/",
            "Tests\\Az\\Session\\": "vendor/az/session/tests/",
            "Tests\\Az\\Validation\\": "vendor/az/validation/tests/",
            "Tests\\Sys\\": "vendor/az/sys/tests/"
        }
    }
}
